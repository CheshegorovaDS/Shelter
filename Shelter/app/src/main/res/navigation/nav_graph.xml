<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/logInAppFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.shelter.presentation.onBoarding.login2.view.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/activity_login">
        <action
            android:id="@+id/action_loginFragment_to_baseFragment2"
            app:destination="@id/logInAppFragment" />
    </fragment>
    <activity
        android:id="@+id/registrationActivity"
        android:name="com.example.shelter.presentation.onBoarding.registration.view.RegistrationActivity"
        android:label="RegistrationActivity"
        tools:layout="@layout/activity_registration"/>
    <fragment
        android:id="@+id/logInAppFragment"
        android:name="com.example.shelter.presentation.log_in_app.view.LogInAppFragment"
        android:label="LogInAppFragment"
        tools:layout="@layout/activity_log_or_reg" >
        <action
            android:id="@+id/action_baseFragment2_to_registrationActivity"
            app:destination="@id/registrationActivity" />
        <action
            android:id="@+id/action_baseFragment2_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
</navigation>